@model App_Ventas.Areas.Inventario.Models.ProductoModelView
<script type="text/javascript">
    $(document).ready(function () {
        Generar();
    });
    function Generar() {
        var Params = {
            ID_SUCURSAL: $('#Hdd_ID_SUCURSAL').val(),
            FLG_SERVICIO: $('#Hdd_FLG_SERVICIO').val(),
        }
        $("#iframeReporte").attr("src", '@Url.Content("~/Recursos/Descargas/DescargarProductoPDF.aspx")?' +$.param(Params));
    }
</script>
<style>
    .modal-dialog2 {
        width: 60%;
        margin: 30px auto
    }
</style>
<div class="modal-dialog2">
    <iframe id="iframeReporte" style="background-color:white;width:99%;height:570px;" src=""></iframe> 
</div>
@Html.Hidden("Hdd_ID_SUCURSAL",Model.ID_SUCURSAL)
@Html.Hidden("Hdd_FLG_SERVICIO",Model.FLG_SERVICIO_INT)

